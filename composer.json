{
    "name": "open-runtimes/open-runtimes",
    "description": "Open Runtimes PHP project for testing. PHPUnit tests can test all runtimes automatically.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eldad Fux",
            "email": "eldad@appwrite.io"
        }
    ],
    "autoload": {
        "psr-4": {}
    },
    "autoload-dev": {
        "psr-4": {"Tests\\": "tests"}
    },
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-json": "*",
        "phpunit/phpunit": "^9.5.13"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}